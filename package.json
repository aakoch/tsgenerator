{
  "name": "@foo-dog/tsgenerator",
  "version": "0.1.0",
  "description": "Convert AST to HTML",
  "scripts": {
    "test": "tap run src/test/*.spec.ts",
    "build": "parcel build --no-cache",
    "check": "tsc --noEmit"
  },
  "exports": {
    "./*": "./dist/*.js",
    "./package.json": "./package.json"
  },
  "types": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "author": "Adam Koch (aakoch)",
  "license": "ISC",
  "devDependencies": {
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@tapjs/tsx": "^1.1.22",
    "@tsconfig/node20": "^20.1.4",
    "@types/debug": "^4.1.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.12.11",
    "parcel": "^2.12.0",
    "tap": "^18.7.3",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@foo-dog/utils": "file:../utils",
    "chalk": "^5.3.0",
    "debug": "^4.3.4",
    "jsdom": "^24.0.0"
  },
  "type": "module",
  "engines": {
    "node": ">=20"
  },
  "tap": {
    "plugin": [
      "@tapjs/tsx",
      "!@tapjs/typescript"
    ]
  },
  "targets": {
    "index": {
      "source": "src/main/index.ts",
      "distDir": "dist",
      "isLibrary": true,
      "optimize": false,
      "context": "node",
      "outputFormat": "esmodule"
    },
    "cli": {
      "source": "src/main/cli.ts",
      "distDir": "dist",
      "isLibrary": true,
      "optimize": false,
      "context": "node",
      "outputFormat": "esmodule"
    }
  }
}
